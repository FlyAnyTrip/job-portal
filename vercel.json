{
  "version": 2,
  "name": "jobportal-backend-api",
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
